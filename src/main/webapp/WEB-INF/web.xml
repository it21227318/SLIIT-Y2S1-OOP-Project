<?xml version="1.0" encoding="UTF-8"?>
<element>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.servlet.loginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/loginServlet</url-pattern>
	</servlet-mapping>
	

	<servlet>
		<servlet-name>signup</servlet-name>
		<servlet-class>com.servlet.signupServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>signup</servlet-name>
		<url-pattern>/signupServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>updatecustomer</servlet-name>
		<servlet-class>com.servlet.updatedetailsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updatecustomer</servlet-name>
		<url-pattern>/updatedetailsServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deletecustomer</servlet-name>
		<servlet-class>com.servlet.deletecustomerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deletecustomer</servlet-name>
		<url-pattern>/deletecustomerServlet</url-pattern>
	</servlet-mapping>
	

	
</web-app>
</element>


